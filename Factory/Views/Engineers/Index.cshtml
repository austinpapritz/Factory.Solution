@{
    ViewData["Title"] = "Engineer Page";
}

<h1>@ViewData["Title"]</h1>

@if (@Model.Count == 0)
{
	<h3>There's nothing here</h3>
}

<ul>
    @foreach (Engineer engineer in Model)
    {
			<li>
                <a href="/engineers/details/@engineer.EngineerId">@engineer.Name</a>
            </li>
    }
</ul>