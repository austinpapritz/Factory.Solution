@{
    ViewData["Title"] = "Engineer Page";
}

<h1>@ViewData["Title"]</h1>

@if (@Model.Count == 0)
{
	<h3>There's nothing here</h3>
}

<ul>
    @foreach (Engineer engineer in Model)
    {
			<li>
                @engineer.Name
                <a href="/engineers/details/@engineer.EngineerId">🔎</a>
                <a href="/engineers/edit/@engineer.EngineerId">✏️</a>
                <a href="#" data-id="@engineer.EngineerId" class="delete-link">❌</a>
            </li>
    }
</ul>